{
	"name": "obsidian-sample-plugin",
	"version": "1.0.0",
	"description": "This is a sample plugin for Obsidian (https://obsidian.md)",
	"main": "main.js",
	"type": "module",
	"scripts": {
		"build": "eslint . && prettier --check . && tsc -noEmit -skipLibCheck && node esbuild.config.mjs",
		"fix": "eslint . --fix && prettier --write .",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"test": "node --import tsx --test tests/**/*.test.ts",
		"package": "node package.mjs",
		"deploy_local": "node deploy-local.mjs"
	},
	"keywords": [],
	"license": "0-BSD",
	"devDependencies": {
		"@eslint/js": "9.30.1",
		"@types/node": "^16.11.6",
		"dotenv": "^17.2.3",
		"esbuild": "0.25.5",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-obsidianmd": "0.1.9",
		"globals": "14.0.0",
		"jiti": "2.6.1",
		"prettier": "^3.7.4",
		"tslib": "2.4.0",
		"tsx": "^4.21.0",
		"typescript": "^5.8.3",
		"typescript-eslint": "8.35.1"
	},
	"dependencies": {
		"obsidian": "latest"
	}
}
